config_version=5

[application]

config/name="Sentou Robo"
run/main_scene="res://Assets/Scenes/Menu.tscn"
config/features=PackedStringArray("4.1", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

AudioManager="*res://Assets/Scripts/Singletons/audio_manager.gd"
ParticleManager="*res://Assets/Scripts/Singletons/particle_manager.gd"
GameManager="*res://Assets/Scripts/Singletons/game_manager.gd"
AttackManager="*res://Assets/Scripts/Singletons/attack_manager.gd"

[debug]

settings/stdout/print_fps=true

[display]

window/size/viewport_width=960
window/size/viewport_height=540
window/stretch/mode="canvas_items"
window/stretch/aspect="expand"

[editor_plugins]

enabled=PackedStringArray("res://addons/proton_scatter/plugin.cfg", "res://addons/terrain_3d/plugin.cfg")

[filesystem]

import/fbx/enabled=false

